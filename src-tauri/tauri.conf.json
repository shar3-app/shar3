{
  "build": {
    "beforeDevCommand": "pnpm dev:web",
    "beforeBuildCommand": "pnpm build:web",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Shar3",
  "mainBinaryName": "Shar3",
  "version": "0.1.0",
  "identifier": "dev.davru.shar3",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Shar3",
        "minWidth": 440,
        "minHeight": 580,
        "width": 525,
        "height": 700,
        "resizable": true,
        "maximizable": false,
        "fullscreen": false,
        "dragDropEnabled": true
      }
    ]
  }
}
